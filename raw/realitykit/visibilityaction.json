  "primaryContentSections": [
    {
      "content": [
        {
          "type": "heading",
          "anchor": "overview",
          "level": 2,
          "text": "Overview"
        },
        {
          "type": "paragraph",
          "inlineContent": [
            {
              "type": "text",
              "text": "Use this action to display or hide a prim with a transform animation. This action is distinct from "
            },
            {
              "code": "UsdGeomImageable",
              "type": "codeVoice"
            },
            {
              "type": "text",
              "text": " because the animation doesn’t alter the "
            },
            {
              "code": "visible",
              "type": "codeVoice"
            },
            {
              "type": "text",
              "text": " property."
            }
          ]
        },
        {
          "type": "heading",
          "anchor": "Declaration",
          "level": 3,
          "text": "Declaration"
        },
        {
          "type": "codeListing",
          "code": [
            "class Preliminary_Action \"VisibilityAction\""
          ],
          "syntax": "other"
        }
      ],
      "kind": "content"
    }
  ],
